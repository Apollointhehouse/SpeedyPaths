{
  "schemaVersion": 1,
  "id": "speedy-paths",
  "version": "${version}",

  "name": "Speedy Paths",
  "description": "A babric mod for BTA 1.7.6.2_02 that makes your legs go brrrr on cobblestone, gravel, and dirt paths.",
  "authors": [
    "Apollointhehouse"
  ],
  "contact": {
    "homepage": "https://github.com/Apollointhehouse/SpeedyPaths",
    "sources": "https://github.com/Apollointhehouse/SpeedyPaths"
  },

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
        {
          "adapter": "kotlin",
          "value": "apollointhehouse.speedyPaths.SpeedyPathsKt::init"
        }
    ]
  },
  "mixins": [
    "speedyPaths.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.7",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
  }
}
